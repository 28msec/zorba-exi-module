                    <xs:element name="administrative_divisions" type="xs:string" />
                    <xs:element name="independence" type="xs:string" />
                    <xs:element name="national_holiday" type="xs:string" />
                    <xs:element name="constitution" type="xs:string" />
                    <xs:element name="legal_system" type="xs:string" />
                    <xs:element name="suffrage" type="xs:string" />
                    <xs:element name="executive_branch">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element maxOccurs="unbounded" name="note" type="xs:string" />
                          <xs:element name="chief_of_state" type="xs:string" />
                          <xs:element name="head_of_government" type="xs:string" />
                          <xs:element name="cabinet" type="xs:string" />
                          <xs:element minOccurs="0" name="elections" type="xs:string" />
                          <xs:element minOccurs="0" name="election_results" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="legislative_branch">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="note" type="xs:string" />
                          <xs:element minOccurs="0" name="elections" type="xs:string" />
                          <xs:element minOccurs="0" name="election_results" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="judicial_branch" type="xs:string" />
                    <xs:element name="political_parties_and_leaders" type="xs:string" />
                    <xs:element name="political_pressure_groups_and_leaders" type="xs:string" />
                    <xs:element name="international_organization_participation" type="xs:string" />
                    <xs:element name="diplomatic_representation_in_the_US">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element minOccurs="0" name="chief_of_mission" type="xs:string" />
                          <xs:element minOccurs="0" name="chancery" type="xs:string" />
                          <xs:element minOccurs="0" name="telephone" type="xs:string" />
                          <xs:element minOccurs="0" name="FAX" type="xs:string" />
                          <xs:element minOccurs="0" name="consulate_general" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="diplomatic_representation_from_the_US">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element minOccurs="0" name="chief_of_mission" type="xs:string" />
                          <xs:element minOccurs="0" name="embassy" type="xs:string" />
                          <xs:element minOccurs="0" name="mailing_address" type="xs:string" />
                          <xs:element minOccurs="0" name="telephone" type="xs:string" />
                          <xs:element minOccurs="0" name="FAX" type="xs:string" />
                          <xs:element minOccurs="0" name="consulate_general" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="flag_description" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="economy">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="economy_overview" type="xs:string" />
                    <xs:element name="GDP" type="xs:string" />
                    <xs:element name="GDP_real_growth_rate" type="xs:string" />
                    <xs:element name="GDP_per_capita" type="xs:string" />
