                    <xs:element name="map_references" type="xs:string" />
                    <xs:element name="area">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="total" type="xs:string" />
                          <xs:element name="land" type="xs:string" />
                          <xs:element name="water" type="xs:string" />
