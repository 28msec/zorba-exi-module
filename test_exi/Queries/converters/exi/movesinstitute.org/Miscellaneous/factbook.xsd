                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="infant_mortality_rate" type="xs:string" />
                    <xs:element name="life_expectancy_at_birth">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element name="total_population" type="xs:string" />
                          <xs:element name="male" type="xs:string" />
                          <xs:element name="female" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="total_fertility_rate" type="xs:string" />
                    <xs:element name="HIV_AIDS-adult_prevalence_rate" type="xs:string" />
                    <xs:element name="HIV_AIDS-people_living_with_HIV_AIDS" type="xs:string" />
                    <xs:element name="HIV_AIDS-deaths" type="xs:string" />
                    <xs:element name="nationality">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="noun" type="xs:string" />
                          <xs:element name="adjective" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ethnic_groups" type="xs:string" />
                    <xs:element name="religions" type="xs:string" />
                    <xs:element name="languages" type="xs:string" />
                    <xs:element name="literacy">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element name="definition" type="xs:string" />
                          <xs:element name="total_population" type="xs:string" />
                          <xs:element name="male" type="xs:string" />
                          <xs:element name="female" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="government">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="country_name">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="conventional_long_form" type="xs:string" />
                          <xs:element maxOccurs="unbounded" name="conventional_short_form" type="xs:string" />
                          <xs:element maxOccurs="unbounded" name="local_long_form" type="xs:string" />
                          <xs:element maxOccurs="unbounded" name="local_short_form" type="xs:string" />
