                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="note" type="xs:string" />
                          <xs:element name="contiguous_zone" type="xs:string" />
                          <xs:element name="continental_shelf" type="xs:string" />
                          <xs:element name="exclusive_economic_zone" type="xs:string" />
                          <xs:element name="territorial_sea" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="climate" type="xs:string" />
                    <xs:element name="terrain" type="xs:string" />
                    <xs:element name="elevation_extremes">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="lowest_point" type="xs:string" />
                          <xs:element name="highest_point" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="natural_resources" type="xs:string" />
                    <xs:element name="land_use">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element maxOccurs="unbounded" name="arable_land" type="xs:string" />
                          <xs:element maxOccurs="unbounded" name="permanent_crops" type="xs:string" />
                          <xs:element maxOccurs="unbounded" name="permanent_pastures" type="xs:string" />
                          <xs:element maxOccurs="unbounded" name="forests_and_woodlands" type="xs:string" />
                          <xs:element maxOccurs="unbounded" name="other_land_uses" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" name="irrigated_land" type="xs:string" />
                    <xs:element name="natural_hazards" type="xs:string" />
                    <xs:element name="current_environment_issues" type="xs:string" />
                    <xs:element name="international_environment_agreements">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="party_to" type="xs:string" />
                          <xs:element name="signed_but_not_ratified" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="geography_note" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="people">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="population" type="xs:string" />
                    <xs:element name="age_structure">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element name="years-0_to_14" type="xs:string" />
                          <xs:element name="years-15_to_64" type="xs:string" />
                          <xs:element name="years-65_and_over" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="population_growth_rate" type="xs:string" />
                    <xs:element name="birth_rate" type="xs:string" />
                    <xs:element name="death_rate" type="xs:string" />
                    <xs:element name="net_migration_rate" type="xs:string" />
                    <xs:element name="sex_ratio">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element name="at_birth" type="xs:string" />
                          <xs:element name="under_15_years" type="xs:string" />
                          <xs:element name="years-15_to_64" type="xs:string" />
                          <xs:element name="years-65_and_over" type="xs:string" />
