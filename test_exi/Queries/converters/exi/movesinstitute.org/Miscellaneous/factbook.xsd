                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="revenues" type="xs:string" />
                          <xs:element name="expenditures" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="industries" type="xs:string" />
                    <xs:element name="industrial_production_growth_rate" type="xs:string" />
                    <xs:element name="electricity_production" type="xs:string" />
                    <xs:element name="electricity_production_by_source">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="fossil_fuel" type="xs:string" />
                          <xs:element name="hydro" type="xs:string" />
                          <xs:element name="nuclear" type="xs:string" />
                          <xs:element minOccurs="0" name="other" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="electricity_consumption" type="xs:string" />
                    <xs:element name="electricity_exports" type="xs:string" />
                    <xs:element name="electricity_imports" type="xs:string" />
                    <xs:element name="agriculture_products" type="xs:string" />
                    <xs:element name="exports" type="xs:string" />
                    <xs:element name="export_commodities" type="xs:string" />
                    <xs:element name="export_partners" type="xs:string" />
                    <xs:element name="imports" type="xs:string" />
                    <xs:element name="import_commodities" type="xs:string" />
                    <xs:element name="import_partners" type="xs:string" />
                    <xs:element name="external_dept" type="xs:string" />
                    <xs:element name="external_aid_recipient" type="xs:string" />
                    <xs:element name="currency" type="xs:string" />
                    <xs:element name="currency_code" type="xs:string" />
                    <xs:element name="exchange_rates" type="xs:string" />
                    <xs:element name="fiscal_year" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="communications">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="telephones_main_lines_in_use" type="xs:string" />
                    <xs:element name="telephones_mobile_cellular" type="xs:string" />
                    <xs:element name="telephone_system">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element name="general_assessment" type="xs:string" />
                          <xs:element name="domestic" type="xs:string" />
                          <xs:element name="international" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="radio_broadcast_stations" type="xs:string" />
                    <xs:element name="radios" type="xs:string" />
                    <xs:element name="television_broadcast_stations" type="xs:string" />
                    <xs:element name="televisions" type="xs:string" />
                    <xs:element name="internet_country_code" type="xs:string" />
                    <xs:element name="internet_service_providers" type="xs:string" />
                    <xs:element name="internet_users" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="transportation">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="railways">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="total" type="xs:string" />
                          <xs:element name="standard_gauge" type="xs:string" />
                          <xs:element name="narrow_gauge" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="highways">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="total" type="xs:string" />
                          <xs:element name="paved" type="xs:string" />
                          <xs:element name="unpaved" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="waterways" type="xs:string" />
                    <xs:element name="pipelines" type="xs:string" />
                    <xs:element name="ports_and_harbors" type="xs:string" />
                    <xs:element name="merchant_marine">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="total" type="xs:string" />
                          <xs:element name="ships_by_type" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="airports" type="xs:string" />
                    <xs:element name="airports_with_paved_runways">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element name="total" type="xs:string" />
                          <xs:element minOccurs="0" name="length_over_3047_meters" type="xs:string" />
                          <xs:element minOccurs="0" name="length_2438__to_3047_meters" type="xs:string" />
                          <xs:element minOccurs="0" name="length_1524__to_2437_meters" type="xs:string" />
                          <xs:element minOccurs="0" name="length_914__to_1523_meters" type="xs:string" />
                          <xs:element minOccurs="0" name="length_under_914_meters" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="airports_with_unpaved_runways">
                      <xs:complexType>
                        <xs:sequence minOccurs="0">
                          <xs:element name="total" type="xs:string" />
                          <xs:element minOccurs="0" name="length_over_3047_meters" type="xs:unsignedByte" />
                          <xs:element minOccurs="0" name="length_2438__to_3047_meters" type="xs:unsignedByte" />
                          <xs:element minOccurs="0" name="length_1524__to_2437_meters" type="xs:string" />
                          <xs:element minOccurs="0" name="length_914__to_1523_meters" type="xs:string" />
                          <xs:element minOccurs="0" name="length_under_914_meters" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="heliports" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="military">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="military_branches" type="xs:string" />
                    <xs:element name="military_age" type="xs:string" />
                    <xs:element name="military_availability" type="xs:string" />
                    <xs:element name="fit_for_military_service" type="xs:string" />
                    <xs:element name="reaching_military_age_annually" type="xs:string" />
                    <xs:element name="military_expenditure_dollar_figure" type="xs:string" />
                    <xs:element name="military_expenditures_percent_GDP" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="transnational_disputes">
                <xs:complexType>
                  <xs:sequence minOccurs="0">
                    <xs:element name="international_disputes" type="xs:string" />
                    <xs:element name="illicit_drugs" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>