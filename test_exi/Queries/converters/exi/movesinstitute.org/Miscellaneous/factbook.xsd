                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="agriculture" type="xs:string" />
                          <xs:element name="industry" type="xs:string" />
                          <xs:element name="services" type="xs:string" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="population_below_poverty_line" type="xs:string" />
